{
  "name": "ramda-repl",
  "contributors": [{
    "name": "Craig Dallimore",
    "email": "",
    "url": ""
  }, {
    "name": "Michael Hurley",
    "email": "mh@buzzdecafe.com",
    "url": "http://buzzdecafe.com"
  }],
  "description": "Ramda + Ramda Fantasy + Sanctuary REPL",
  "version": "1.0.0",
  "dependencies": {
    "babel-core": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babelify": "7.3.0",
    "babylon": "6.11.2",
    "browserify": "^13.1.0",
    "debounce": "1.0.0",
    "load-script": "^1.0.0",
    "pretty-js": "^0.1.8",
    "ramda": "0.22.1",
    "ramda-fantasy": "0.7.0",
    "rdom": "^0.6.0",
    "sanctuary": "0.11.1",
    "through2": "2.0.1",
    "vinyl-fs": "2.4.3"
  },
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/ramda/ramda.github.io.git"
  },
  "scripts": {
    "build": "npm run build-js; npm run build-css; npm run minify",
    "build-js": "browserify -t [ babelify ] ./lib/js/main.js -o ./dist/ramdaRepl.js",
    "watch-js": "watchify -t [ babelify ] ./lib/js/main.js -dvo 'exorcist ./dist/ramdaRepl.js.map > ./dist/ramdaRepl.js'",
    "build-css": "postcss -u postcss-import -u cssnano -u autoprefixer 'last 2 versions' -o ./dist/ramdaRepl.css ./lib/css/repl.css",
    "watch-css": "postcss -u postcss-import -u cssnano -u autoprefixer 'last 2 versions' --watch -o ./dist/ramdaRepl.css ./lib/css/repl.css",
    "watch": "npm run watch-css & npm run watch-js",
    "minify": "npm run build-js && uglifyjs ./dist/ramdaRepl.js -o ./dist/ramdaRepl.min.js",
    "test": "mocha",
    "server": "http-server",
    "lint": "eslint lib/js/*.js; eslint test/*.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "codemirror": "^5.19.0",
    "cssnano": "^3.7.5",
    "eslint": "^3.7.0",
    "exorcist": "^0.4.0",
    "fake-xml-http-request": "^1.4.0",
    "http-server": "^0.9.0",
    "jsdom": "^9.5.0",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.1.0",
    "mock-require": "^1.3.0",
    "postcss": "^5.2.4",
    "postcss-cli": "^2.6.0",
    "postcss-import": "^8.1.2",
    "sinon": "^1.17.6",
    "uglify-js": "^2.7.3",
    "watchify": "^3.7.0"
  }
}
