{
  "name": "ramda-repl",
  "description": "Ramda + Ramda Fantasy + Sanctuary REPL",
  "version": "1.0.0",
  "dependencies": {
    "babel-core": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babelify": "7.3.0",
    "babylon": "6.11.2",
    "browserify": "^13.1.0",
    "debounce": "1.0.0",
    "pretty-js": "^0.1.8",
    "ramda": "0.22.1",
    "ramda-fantasy": "0.7.0",
    "rdom": "^0.6.0",
    "sanctuary": "0.11.1",
    "through2": "2.0.1",
    "vinyl-fs": "2.4.3"
  },
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
